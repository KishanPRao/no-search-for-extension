{
  "manifest_version": 2,
  "name": "No Search For",
  "version": "1",
  "description": "Remove the 'People also search for' , 'People also ask' and 'featured snippets' elements on google search",
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "include_globs": [
        "http://www.google.*/*",
        "https://www.google.*/*"
      ],
      "css": [
        "root.css"
      ]
    }
  ],
  "incognito": "spanning"
}